@misc{_cis-regulatory_2012,
	title = {Cis-regulatory module},
	copyright = {Creative Commons Attribution-{ShareAlike} License},
	url = {http://en.wikipedia.org/w/index.php?title=Cis-regulatory_module&oldid=508130143},
	abstract = {Cis-regulatory module ({CRM)} is a stretch of {DNA}, usually 100-1000 {DNA} base pairs in length,[1] where a number of transcription factors can bind and regulate expression of nearby genes. One cis-regulatory element can regulate several genes,[2] and conversely, one gene can have several cis-regulatory modules.[3] Cis-regulatory modules are one of several types of functional regulatory elements. Regulatory elements are binding sites for transcription factors, which are involved in gene regulation.[1] Cis-regulatory modules perform a large amount of developmental information processing.[1] Cis-regulatory modules are non-random clusters at their specified target site that contain transcription factor binding sites.[1] They are labeled as cis because they are typically located on the same {DNA} as the genes they control as opposed to trans, which refers to effects on genes not located on the same strand or farther away, such as transcription factors.[1] The original definition presented cis-regulatory modules as enhancers of cis-acting {DNA}, which increased the rate of transcription from a linked promoter.[4] However, this definition has changed to define cis-regulatory modules as a {DNA} sequence with transcription factor binding sites which are clustered into modular structures, including -but not limited to- locus control regions, promoters, enhancers, silencers, boundary control elements and other modulators.[4] Cis-regulatory modules can be divided into classes. Enhancers regulate gene expression positively.[1] Insulators work indirectly by interacting with other nearby cis-regulatory modules.[1] Silencers act by silencing genes.[1] Cis-regulatory modules carry out their function by integrating the active transcription factors and the associated co-factors at a specific time and place in the cell, this information is read and an output is given.[4]},
	language = {en},
	urldate = {2012-09-24},
	journal = {Wikipedia, the free encyclopedia},
	month = sep,
	year = {2012},
	note = {Page Version {ID:} 508130143},
	file = {Snapshot:C:\Documents and Settings\Colin Diesh\Application Data\Mozilla\Firefox\Profiles\v9bgvjx6.default\zotero\storage\XPRRKH5N\index.html:text/html}
},

